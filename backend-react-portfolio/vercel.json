{
  "version": 2,
  "builds": [
    { "src": "api/index.js", "use": "@vercel/node" },
    {
      "src": "frontend-react-portfolio/package.json",
      "use": "@vercel/static-build",
      "config": { "distDir": "frontend-react-portfolio/build" }
    }
  ],
  "routes": [
    { "src": "/", "dest": "api/index.js" },
    { "src": "/api/(.*)", "dest": "api/index.js" },
    { "src": "/(.*)", "dest": "/frontend-react-portfolio/build/$1" }
  ]
}
