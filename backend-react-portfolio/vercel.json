{
  "version": 2,
  "builds": [
    { "src": "api/index.js", "use": "@vercel/node" },
    { "src": "frontend-react-portfolio/build/**/*", "use": "@vercel/static" }
  ],
  "routes": [
    { "src": "/", "dest": "api/index.js" },
    { "src": "/api/(.*)", "dest": "api/index.js" },
    {
      "src": "/static/(.*)",
      "dest": "/frontend-react-portfolio/build/static/$1"
    },
    { "src": "/(.*)", "dest": "/frontend-react-portfolio/build/index.html" }
  ]
}
